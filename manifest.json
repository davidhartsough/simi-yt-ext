{
  "manifest_version": 3,
  "name": "SiMi YT",
  "version": "1.1",
  "description": "Simplistic Minimalistic YouTube",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "css": ["simi-yt.css"],
      "js": ["simi-yt.js"],
      "matches": ["https://www.youtube.com/*"]
    }
  ]
}
